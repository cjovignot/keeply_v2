{
  "name": "keeply-monorepo",
  "private": true,
  "workspaces": [
    "api",
    "frontend"
  ],
  "scripts": {
    "dev:client": "npm --prefix frontend run dev",
    "dev:api": "npm --prefix api run dev",
    "dev": "concurrently  \"npm run dev:api\" \"npm run dev:client\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
